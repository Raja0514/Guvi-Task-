//1.sum of all numbers in array using anonymous function &&IIFE function 
------------------------------------------------------------------------------------------------------------------
//anaymous function

var anmous=function(){
var sum=0;
const arr=[1,2,3,5,6,6,99,100,2500];
for(var i=0;i<arr.length;i++){
sum=sum+arr[i];
}
console.log(sum);
}
anmous();
-----------------------------------------------------------------------------------------------------------------------------------
//IIFE function

(function(){
var sum=0;
 const arr=[1,2,3,5,6,6,99,100,2500,1500,2500];
for(var i=0;i<arr.length;i++){
sum=sum+arr[i];
}
console.log(sum);   
})
();
-----------------------------------------------------------------------------------------------------------------------------------------------------
//2.print odd numbers in array
---------------------------------------------------------------------------------------------------------------------------------------------------
//anonymous function

var odd=function(){
var odd=[1,2,3,4,5,6,7,8];
for(var i=0;i<odd.length;i++){
if(odd[i]%2==1){
console.log(odd[i]);
}
}}
odd();
--------------------------------------------------------------------------------------------------------------------------------------------------------------
//IIFE function

(function(){
    var odd1=[4,9,10,7,13];
    for (var i=0;i<odd1.length;i++){
        if(odd1[i]%2==1){
            console.log(odd1[i]);
            }
            }
})
();


--------------------------------------------------------------------------------------------------------------------------------------
